{
	"files.exclude": {
		"pnpm-lock.yaml": true,
		".vscode": true,
		"example/.gitignore": true,
		"example/components.theme.dark.json": true,
		"example/pnpm-lock.yaml": true,
		"example/README.md": true,
		"example/node_modules": true,
		"example/.vscode": true,
		".history": true,
		".cache": true,
		"src/env.d.ts": true,
		".gitignore": true,
		".npmrc": true,
		"deno.jsonc": true,
		"dprint.json": true,
		"README.md": true,
		".lh": true,
		"node_modules": true,
		"tsconfig.json": true,
		".depcheckrc": true,
		".editorconfig": true,
		".npmcheckrc": true
	},
	"deno.enable": false,
	"cSpell.words": [
		"DANGI",
		"Moby"
	],
	"editor.fontFamily": "Monaco, 'Andale Mono', monospace",
	"editor.codeLensFontFamily": "Monaco, 'Andale Mono', monospace",
	"editor.inlayHints.fontFamily": "Monaco, 'Andale Mono', monospace",
	"editor.tabSize": 2,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "dprint.dprint",
	"local-history.date.dateFormat": "DD-MM-YYYY hh:mm",
	"typescript.format.semicolons": "remove",
	"javascript.format.semicolons": "remove",
	"yaml.format.proseWrap": "always"
}
