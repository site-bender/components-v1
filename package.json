	"scripts": {
		"build": "astro build",
		"check:exports": "npx find-unused-exports",
		"check:packages": "npx npm-check -u",
		"config:update": "dprint config update",
		"dep:dev": "pnpm fmt && pnpm lint && pnpm check:exports && pnpm check:packages && pnpm build && rm -rf .cache .history dist && git add -A",
		"dep:prod": "pnpm dep:dev && vercel deploy",
		"dev": "cd example && astro dev",
		"fmt": "dprint fmt",
		"fmt:check": "dprint check",
		"lint": "deno lint",
		"preview": "astro preview",
		"start": "astro dev",
		"test": "pnpm test:int",
		"test:cov": "vitest run --coverage",
		"test:int": "vitest"
	},
	"dependencies": {
		"astro": "^2.9.1"
	},
	"devDependencies": {
		"@astrojs/mdx": "^0.19.7",
		"@js-temporal/polyfill": "^0.4.4",
